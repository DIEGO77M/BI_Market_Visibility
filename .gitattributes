# Databricks notebooks in Python format
notebooks/*.py linguist-language=Python linguist-detectable=true
notebooks/*.py linguist-documentation=false

# Data files
*.csv filter=lfs diff=lfs merge=lfs -text
*.xlsx filter=lfs diff=lfs merge=lfs -text
*.parquet filter=lfs diff=lfs merge=lfs -text

# Jupyter notebooks
*.ipynb linguist-language=Jupyter-Notebook linguist-detectable=true
